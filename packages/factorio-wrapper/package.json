{
  "name": "@proj-airi/factorio-wrapper",
  "type": "module",
  "version": "0.1.0",
  "description": "Run factorio server and convert std stream to websocket stream",
  "author": "LemonNekoGH",
  "license": "MIT",
  "keywords": [
    "factorio",
    "websocket",
    "typescript"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "dotenvx run -f .env -f .env.local --overload --debug -- tsx src/main.ts",
    "start": "dotenvx run -f .env -f .env.local --overload -- tsx src/main.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.51.0",
    "@guiiai/logg": "^1.2.4",
    "execa": "^9.6.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/node": "^22.18.12",
    "@types/ws": "^8.18.1",
    "tsx": "^4.20.6",
    "unbuild": "^3.6.1",
    "vitest": "^4.0.4"
  }
}
